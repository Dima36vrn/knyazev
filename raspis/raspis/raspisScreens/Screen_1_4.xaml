<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:pi="http://schemas.microsoft.com/prototyping/2010/interactivity" xmlns:local="clr-namespace:raspisScreens" x:Name="Screen_1_4_Name"
	mc:Ignorable="d"
	x:Class="raspisScreens.Screen_1_4"
	Width="632" Height="328">
	<UserControl.Background>
		<ImageBrush ImageSource="Снимок.JPG" Stretch="Uniform"/>
	</UserControl.Background>

	<Grid x:Name="LayoutRoot">
		<Grid.Background>
			<ImageBrush/>
		</Grid.Background>
		<Button Content="Открыть" HorizontalAlignment="Right" Height="40" Margin="0,0,40,16" VerticalAlignment="Bottom" Width="80" Background="#A5FF0909">
			<i:Interaction.Triggers>
				<i:EventTrigger EventName="Click">
					<pi:NavigateToScreenAction TargetScreen="raspisScreens.Screen_1_1"/>
				</i:EventTrigger>
			</i:Interaction.Triggers>
		</Button>
		<CheckBox Content="Выбрать" HorizontalAlignment="Right" Height="24" Margin="0,96,24,0" VerticalAlignment="Top" Width="96" FontSize="13.333" FontWeight="Bold"/>
		<local:Screen_1_2 HorizontalAlignment="Left" VerticalAlignment="Top" d:IsPrototypingComposition="True" Background="#59F7002D"/>
	</Grid>
</UserControl>